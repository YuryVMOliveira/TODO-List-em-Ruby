<h1>Login</h1>
<%= form_with url: sessions_path, local: true do |form| %>
  <div>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </div>
  <div>
    <%= form.label :password %><br>
    <%= form.password_field :password %>
  </div>
  <%= form.submit "Login" %>
<% end %>
<p> Don't have an account? <%= link_to "Register", new_user_path %></p>